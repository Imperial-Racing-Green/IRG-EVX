function [F_x,F_y,F_xmax,F_y] = tyre_fmax(Fz,mesh)

SL = linspace(-1,1,mesh);

SA = linspace(-30,30,mesh);

[SA,SL] = meshgrid(SA,SL);

[F_x,F_y,~] = PacejkaTest(SA,SL,Fz);

figure
surf(SA,SL,F_y)
figure
hold on
for i = 1:length(F_x)
    for j = 1:length(F_x)
        scatter(F_x(i,j),F_y(i,j))
    end
end
xlabel('Fx')
ylabel('Fy')

